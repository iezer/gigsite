<h1>Listing venues</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Main url</th>
    <th>Access url</th>
    <th>Geo x</th>
    <th>Geo y</th>
    <th>Ranking</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @venues.each do |venue| %>
  <tr>
    <td><%= venue.name %></td>
    <td><%= venue.address %></td>
    <td><%= venue.main_url %></td>
    <td><%= venue.access_url %></td>
    <td><%= venue.geo_x %></td>
    <td><%= venue.geo_y %></td>
    <td><%= venue.ranking %></td>
    <td><%= venue.description %></td>
    <td><%= link_to 'Show', venue %></td>
    <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
    <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Venue', new_venue_path %>
