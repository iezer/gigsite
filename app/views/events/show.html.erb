<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @event.name %>
</p>

<p>
  <b>Id:</b>
  <%= @event.id %>
</p>

<p>
  <b>Venue:</b>
  <%= @event.venue_id %>
</p>

<p>
  <b>Start:</b>
  <%= @event.start %>
</p>

<p>
  <b>End:</b>
  <%= @event.end %>
</p>

<p>
  <b>Info url:</b>
  <%= @event.info_url %>
</p>

<p>
	<p>Musicians:</b>
		<ol>
		<% @event.musicians.each do |m| %>	
			<li><%= m.name %></li>
		<% end %>	
		</ol>

</p>

<%= link_to 'Edit', edit_event_path(@event) %> |
<%= link_to 'Back', events_path %>
